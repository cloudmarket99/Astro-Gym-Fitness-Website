---
import Layout from '../layouts/Layout.astro';
import BatteryHero from '../components/BatteryHero.astro';
import Statistics from '../components/Statistics.astro';
import BatteryBrands from '../components/BatteryBrands.astro';
import BatteryServices from '../components/BatteryServices.astro';
import BatteryFeatures from '../components/BatteryFeatures.astro';
import BatteryProcess from '../components/BatteryProcess.astro';
import BatteryReferences from '../components/BatteryReferences.astro';
import BatteryTestimonials from '../components/BatteryTestimonials.astro';
import BatteryFAQ from '../components/BatteryFAQ.astro';
import BatteryCTA from '../components/BatteryCTA.astro';
import { siteData } from '../data/siteData';
---

<Layout title={`${siteData.name} - ${siteData.tagline}`} description={siteData.description}>
  <BatteryHero />
  <Statistics />
  <BatteryBrands />
  <BatteryServices />
  <BatteryFeatures />
  <BatteryProcess />
  <BatteryReferences />
  <BatteryTestimonials />
  <BatteryFAQ />
  <BatteryCTA />
</Layout>
